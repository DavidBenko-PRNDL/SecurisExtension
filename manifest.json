 {
	"manifest_version": 2,

	"name": "Securis",
	"description": "Autofill, copy, and create SeCuris Accounts via Chrome",
	"version": "0.0.1",

	"permissions": [
		"contextMenus",
		"tabs"
	],

	"background": {
		"scripts": [
			"background.js"]
	},

	"content_scripts": [{
		"matches": ["<all_urls>"],
		"exclude_matches": ["https://securis-debug.herokuapp.com/"],
		"js": ["jquery.min.js", "autofill.js"]
	}],

	"browser_action": {
		"default_icon": "icon.png"
	}
}
