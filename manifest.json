{
	"manifest_version": 2,

	"name": "Securis",
	"description": "Autofill, copy, and create Seuris Accounts via Chrome",
	"version": "0.0.1",

	"permissions": [
		"contextMenus",
		"https://*/*"
	],
	"background": {
		"persistent": false,
		"scripts": ["sample.js"]
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}
